generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email        String   @unique
  name         String?
  password     Bytes?
  bod          DateTime?
  create_time  DateTime @default(now()) @db.Timestamptz(6)
  update_time  DateTime @default(now()) @db.Timestamptz(6)
}